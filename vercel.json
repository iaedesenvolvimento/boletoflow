{
    "rewrites": [
        {
            "source": "/assets/(.*)",
            "destination": "/assets/$1"
        },
        {
            "source": "/sw.js",
            "destination": "/sw.js"
        },
        {
            "source": "/favicon.ico",
            "destination": "/favicon.ico"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}